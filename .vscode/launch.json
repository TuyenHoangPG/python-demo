  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "src/app.py",
        "FLASK_ENV": "development",
        "DATABASE_URL": "postgresql://postgres:123456@localhost:5432/python-demo",
        "SECRET_KEY": "secret_key_123$%^&*()",
        "REDISTOGO_URL": "redis://localhost:6379"
      },
      "args": [
        "run",
        "--no-debugger"
      ],
      "jinja": true,
      "justMyCode": true
    }
  ]
}